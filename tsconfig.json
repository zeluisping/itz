{
    "compilerOptions": {
        "lib": ["es5", "es6"],
        "outDir": "lib",
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "importHelpers": true,
        "sourceMap": true,
        "noImplicitAny": true,
        "declaration": true,
        "declarationMap": true,
        "noFallthroughCasesInSwitch": false,
        "noImplicitReturns": true,
        "stripInternal": true,
        "pretty": true,
        "strictNullChecks": true,
        "noUnusedLocals": true,
        "downlevelIteration": true
    },
    "include": ["src"],
    "exclude": ["node_modules"]
}
